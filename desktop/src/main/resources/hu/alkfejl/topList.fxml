<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="hu.alkfejl.controller.TopListController">
    <TabPane AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
             AnchorPane.rightAnchor="0.0">
        <Tab fx:id="onePlayer" closable="false" text="Egy játékos">
            <fx:include fx:id="onePlayerTab" source="onePlayerTab.fxml"/>
        </Tab>
        <Tab fx:id="twoPlayer" closable="false" text="Két játékos">
            <fx:include fx:id="twoPlayerTab" source="twoPlayerTab.fxml"/>
        </Tab>
    </TabPane>
</AnchorPane>